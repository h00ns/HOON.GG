{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    "@emotion/babel-plugin",
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "@styles": "./styles",
          "@components": "./components",
          "@utils": "./utils",
          "@apis": "./apis",
          "@hooks": "./hooks",
          "@public": "./public",
          "@static": "./static",
          "@rcoil": "./recoil",
          "@constants": "./constants",
          "@customType": "./customType"
        }
      }
    ]
  ]
}